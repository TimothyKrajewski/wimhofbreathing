{
    "expo": {
      "name": "WimHofBreathingApp",
      "slug": "wim-hof-breathing-app",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "updates": {
        "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": [
        "**/*"
      ],
      "ios": {
        "supportsTablet": true
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./assets/adaptive-icon.png",
          "backgroundColor": "#ffffff"
        },
        "package": "com.yourcompany.wimhofbreathingapp"
      },
      "web": {
        "favicon": "./assets/favicon.png"
      },
      "packagerOpts": {
        "sourceExts": ["js", "json", "ts", "tsx", "jsx", "vue"]
      }
    }
  }
  